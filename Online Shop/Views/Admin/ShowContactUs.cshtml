        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    </head>
        @model List<UsersViewModels>
    <body>



        <!-- Topbar Start -->
    <div class="container-fluid">
        <div class="row bg-secondary py-1 px-xl-5">
          @*   <div class="col-lg-6 d-none d-lg-block">
                <div class="d-inline-flex align-items-center h-100">
                    <a class="text-body mr-3" href="">About</a>
                    <a class="text-body mr-3" href="">Contact</a>
                    <a class="text-body mr-3" href="">Help</a>
                    <a class="text-body mr-3" href="">FAQs</a>
                </div>
            </div> *@
            <div class="col-lg-6 text-center text-lg-right">
                <div class="d-inline-flex align-items-center">
                    <div class="btn-group">
                        <button type="button" class="btn btn-sm btn-light dropdown-toggle" data-toggle="dropdown">
                            @if (User.Identity.IsAuthenticated)
                            {
                                @(User.Identity.Name)
                            }
                            else
                            {
                                @("My Account")
                            }
                        </button>
                        @{
                            @if (User.Identity.IsAuthenticated)
                            {
                                <div class="dropdown-menu dropdown-menu-right">
                                    <a asp-controller="Account" asp-action="Logout"><button class="dropdown-item" type="button">Logout</button></a>

                                </div>
                            }
                            else
                            {
                                <div class="dropdown-menu dropdown-menu-right">
                                    <a asp-controller="Home" asp-action="SignIn"><button class="dropdown-item" type="button">Sign in</button></a>
                                    <a asp-controller="Home" asp-action="SignUp"> <button class="dropdown-item" type="button">Sign up</button></a>
                                </div>

                            }
                        }
                    </div>


                </div>
                @*   <div class="d-inline-flex align-items-center d-block d-lg-none">
                    <a href="" class="btn px-0 ml-2">
                        <i class="fas fa-heart text-dark"></i>
                        <span class="badge text-dark border border-dark rounded-circle" style="padding-bottom: 2px;">0</span>
                    </a>
                    <a href="" class="btn px-0 ml-2">
                        <i class="fas fa-shopping-cart text-dark"></i>
                        <span class="badge text-dark border border-dark rounded-circle" style="padding-bottom: 2px;">0</span>
                    </a>
                </div> *@
            </div>
        </div>
        <div class="row align-items-center bg-light py-3 px-xl-5 d-none d-lg-flex">
            <div class="col-lg-4">
                <a asp-controller="Admin" asp-action="Index" class="text-decoration-none">
                    <span class="h1 text-uppercase text-primary bg-dark px-2">Multi</span>
                    <span class="h1 text-uppercase text-dark bg-primary px-2 ml-n1">Shop</span>
                </a>
            </div>
            <div class="col-lg-4 col-6 text-left">
                <form action="">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="Search for products">
                        <div class="input-group-append">
                            <span class="input-group-text bg-transparent text-primary">
                                <i class="fa fa-search"></i>
                            </span>
                        </div>
                    </div>
                </form>
            </div>
            <div class="col-lg-4 col-6 text-right">
                <p class="m-0">Customer Service</p>
                <h5 class="m-0">09305507202</h5>
            </div>
        </div>
    </div>
    <!-- Topbar End -->
    <!-- Navbar Start -->
    <div class="container-fluid bg-dark mb-30">
        <div class="row px-xl-5">
            <div class="col-lg-3 d-none d-lg-block">
                <a class="btn d-flex align-items-center justify-content-between bg-primary w-100" data-toggle="collapse" href="#navbar-vertical" style="height: 65px; padding: 0 30px;">
                    <h6 class="text-dark m-0"><i class="fa fa-bars mr-2"></i>Categories</h6>

                </a>

            </div>
            <div class="col-lg-9">
                <nav class="navbar navbar-expand-lg bg-dark navbar-dark py-3 py-lg-0 px-0">
                    <a asp-controller="Admin" asp-action="Index" class="text-decoration-none d-block d-lg-none">
                        <span class="h1 text-uppercase text-dark bg-light px-2">Multi</span>
                        <span class="h1 text-uppercase text-light bg-primary px-2 ml-n1">Shop</span>
                    </a>
                    <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-between" id="navbarCollapse">
                        <div class="navbar-nav mr-auto py-0">
                            <a asp-controller="Admin" asp-action="Index" class="nav-item nav-link">Dashboard</a>
                            <a asp-controller="Admin" asp-action="Insert" class="nav-item nav-link">Insert</a>
                            <a asp-controller="Admin" asp-action="AddCategory" class="nav-item nav-link">Insert Category</a>
                            <a asp-controller="Admin" asp-action="AddVariant" class="nav-item nav-link">Insert Variant</a>

                            <a asp-controller="Admin" asp-action="ShowProducts" class="nav-item nav-link">Products</a>

                            <a asp-controller="Admin" asp-action="ListOfUsers" class="nav-item nav-link">Users</a>
                            <a asp-controller="Admin" asp-action="Orders" class="nav-item nav-link">Orders</a>
                            <a asp-controller="Admin" asp-action="ShowContactUs" class="nav-item nav-link">Contacts</a>
                        </div>
                        @*   <div class="navbar-nav ml-auto py-0 d-none d-lg-block">
                            <a href="" class="btn px-0">
                                <i class="fas fa-heart text-primary"></i>
                                <span class="badge text-secondary border border-secondary rounded-circle" style="padding-bottom: 2px;">0</span>
                            </a>
                            <a href="" class="btn px-0 ml-3">
                                <i class="fas fa-shopping-cart text-primary"></i>
                                <span class="badge text-secondary border border-secondary rounded-circle" style="padding-bottom: 2px;">0</span>
                            </a>
                        </div> *@
                    </div>
                </nav>
            </div>
        </div>
    </div>
    <!-- Navbar End -->
    <!-- Users table start -->
    <div class="container col-2  div-sidebar bg-dark">

        <p class="p-side" style="text-align:center;padding:20px;color:white;">Multi Shop</p>
        <hr style="background-color:white" />
        <ul class="ul-side">
            <li class="side-li"><i class='fas fa-align-left' style="color:black;margin-right:10px;font-size:20px"></i><a asp-controller="Admin" asp-action="Index" class="a-side">Dashboard</a></li>
            <li class="side-li"><i class='fas fa-project-diagram' style="color:black;font-size:20px;margin-right:10px"></i><a href="#" class="a-side">Projects</a></li>
            <li class="side-li"><i class='fas fa-chart-line' style="color:black;margin-right:10px;font-size:20px"></i><a href="#" class="a-side">Analytics</a></li>
            <li class="side-li"><i class='fas fa-paperclip' style="color:black;margin-right:10px;font-size:20px"></i><a href="#" class="a-side">Reports</a></li>
            <li class="side-li"><i class='fas fa-shopping-cart' style="color:black;margin-right:10px;font-size:20px"></i><a href="#" class="a-side">Store Mng</a></li>
            <li class="side-li"><i class='fas fa-gift' style="color:black;margin-right:10px;font-size:20px"></i><a href="#" class="a-side">Products</a></li>
            <li class="side-li"><i class='fas fa-comment-dots' style="margin-right:10px;color:black;font-size:20px"></i><a href="#" class="a-side">Chat</a></li>
            <li class="side-li"><i class='fas fa-map-marker-alt' style="color:black;margin-right:10px;font-size:20px"></i><a href="#" class="a-side">Outlet</a></li>
            <li class="side-li"><i class='fas fa-power-off' style='font-size:20px;color:red;margin-right:10px'></i><a asp-controller="Account" asp-action="Logout" class="a-side">Logout</a></li>
        </ul>

    </div>
    <div class="container col-10 div-mid-body ">
        <div>
            <i class='fas fa-align-left' style="color:black;margin-right:10px;font-size:40px"></i><h1 class="h1-dashboard">Contacts</h1>
        </div>





        <div class="container bg-dark" style="padding:10px;border:2px solid gray;border-radius:10px;margin-top:150px;overflow-x:auto">
            <p style="color:white;font-size:30px">Contacts:</p>
            <hr />
            <table class="table table-bordered">
                <thead style="color:white">
                    <tr>
                        <th style="text-align:center">#</th>
                        <th style="text-align:center">Name</th>
                        <th style="text-align:center">Email</th>
                        <th style="text-align:center">Subject</th>
                        <th style="text-align:center">Message</th>

                    </tr>
                </thead>
                <tbody style="color:white">
                    @{
                        var i = 1;
                        foreach (var item in ViewData["Contacts"] as List<ContactUs>)
                        {


                            <tr>
                                <td style="text-align:center;align-content:center">@i</td>
                                <td style="text-align:center;align-content:center">@item.Name</td>
                                <td style="text-align:center;align-content:center">@item.Email</td>
                                <td style="text-align:center;align-content:center">@item.Subject</td>
                                <td style="text-align:center;align-content:center">@item.Message</td>

                            </tr>
                            i++;
                        }
                    }

                </tbody>
            </table>
        </div>



    </div>
    <!-- Users table end -->
    <!-- Back to Top -->
    <a href="#" class="btn btn-primary back-to-top"><i class="fa fa-angle-double-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
    <script src="~/Layout/lib/easing/easing.min.js"></script>
    <script src="~/Layout/lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Contact Javascript File -->
    <script src="~/Layout/mail/jqBootstrapValidation.min.js"></script>
    <script src="~/Layout/mail/contact.js"></script>

    <!-- Template Javascript -->
    <script src="~/Layout/js/main.js"></script>
    <script src="~/js/JavaScript.js"></script>









    <script src="../../adminlay/plugins/jQuery/jQuery-2.1.3.min.js"></script>
    <!-- Bootstrap 3.3.2 JS -->
    <script src="../../adminlay/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <!-- ChartJS 1.0.1 -->
    <script src="../../adminlay/plugins/chartjs/Chart.min.js" type="text/javascript"></script>
    <!-- FastClick -->
    <script src='../../adminlay/plugins/fastclick/fastclick.min.js'></script>
    <!-- AdminLTE App -->
    <script src="../../adminlay/dist/js/app.min.js" type="text/javascript"></script>
